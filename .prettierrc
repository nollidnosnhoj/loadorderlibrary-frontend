{
	"useTabs": true,
	"tabWidth": 4,
	"semi": true,
	"singleQuote": true,
	"trailingComma": "all",
	"printWidth": 120,
	"plugins": ["prettier-plugin-svelte", "prettier-plugin-tailwindcss"],
	"arrowParens": "always",
	"bracketSpacing": true,
	"overrides": [{ "files": "*.svelte", "options": { "parser": "svelte" } }]
}
